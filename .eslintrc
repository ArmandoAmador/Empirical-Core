{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:all", "plugin:react/all", "airbnb"],
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "comma-dangle": ["error", {
        "arrays": "never",
        "objects": "always",
        "imports": "ignore",
        "exports": "never",
        "functions": "ignore",
    }],
    'no-extend-native': ["warn"],
  }
}
